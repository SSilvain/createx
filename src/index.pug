		.values
			.values__container
				h2.container-title.values__title Our core values
				h3.container-sub-title.values__sub-title Our mission is to set the highest standards for construction sphere.
				.values__body
					each item, i in ["1", "2", "3"]
						.values__item
							img.values__ico(src=["@img/values-ico-1.svg","@img/values-ico-2.svg","@img/values-ico-3.svg",][i])
							h3.values__title-item= ["Quality","Safety","Comfort"][i]
							p.values__text-item= ["Culpa nostrud commodo ea consequat aliquip reprehenderit. Veniam velit nostrud aliquip sunt.","Anim reprehenderit sint voluptate exercitation adipisicing laborum adipisicing. Minim empor est ea.","Sit veniam aute dolore adipisicing nulla sit culpa. Minim mollit voluptate ullamco proident ea ad."][i]
			.values__container--wrapper-form
				h3.ineer-title.values__inner-title Want to know more? Ask us a question:
				form.values__form
					.values__item-form
						label.values__label Name
						input.values__input(type=text, placeholder="Your name")
					.values__item-form
						label.values__label Phone
						input.values__input(type=tel, placeholder="Your phone")
					.values__item-form
						label.values__label Message
						input.values__input(type=message, placeholder="Your message")
					.values__item-form
						button.values__button(type="submit") Send
		.services
			.services__container
				h2.container-title.services__title Our services
				h3.container-sub-title.services__sub-title Createx Construction Bureau is a construction giant with a full range of construction services.
				.services__wrapper
					each item, i in ["Construction","Project Development","Interior Design","Repairs"]
						.services__item
							.services__item-body
								.services__item-ico
									svg
										use(xlink:href=`@img/services-ico/services-ico-sprite.svg#services-ico-${i+1}`)
								.services__item-title= item
				.container__bottom.services__bottom
					.container__bottom-title Learn more about our services
					a.container__bottom-link-button(href="#") View services
		.projects
			.projects__container
				.projects__title.container-title.container-title--left Browse our selected projects and learn more about our work
				.projects__slider-box
					.projects__slider.swiper-projects
						.projects__wrapper.swiper-wrapper
							each item, i in ["Cubes Building","Red Finger Building","The Pencil Building","Red Finger Building","The Pencil Building"]
								.projects__slide.swiper-slide-projects.swiper-slide
									.projects__slide-body
										.projects__slide-wrapper
											.projects__slide-img-container 
												img(src=["@img/projects-slider/projects-1.jpg","@img/projects-slider/projects-2.jpg","@img/projects-slider/projects-3.jpg","@img/projects-slider/projects-2.jpg","@img/projects-slider/projects-3.jpg"][i], alt="projects")
											.projects__slide-content
												.projects__slide-text-wrapper
													.projects__slide-title= item
													.projects__slide-sub-title= ["Business Centers","Business Centers","Stores & Malls","Business Centers","Stores & Malls"][i]
												.projects__slide-button
													button(type=button) View Project
					button.swiper-button-prev-projects(type="button")
						svg
							use(xlink:href="@img/arrow-slider.svg#arrow-slider")
					button.swiper-button-next-projects(type="button")
						svg
							use(xlink:href="@img/arrow-slider.svg#arrow-slider")
				.container__bottom.projects__bottom
					.container__bottom-title Explore all our works
					a.container__bottom-link-button(href="#") View portfolio
